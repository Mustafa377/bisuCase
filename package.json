  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name cbcinputvue ./src/components/CBCInputVue.vue",
    "test": "vue-cli-service serve --mode test",
    "test:build": "vue-cli-service build --mode test",
    "production": "vue-cli-service serve --mode production",
    "production:build": "vue-cli-service build --mode production",
    "s3:test": "aws s3 cp dist/ s3://bisigorta-test-admin/ --recursive --exclude 'vendor/fontawesome-pro*' --profile metal",
    "s3:production": "aws s3 cp dist/ s3://bisigorta-admin/ --recursive --exclude 'vendor/fontawesome-pro*' --profile metal",
    "deploy:test": "npm run test:build && npm run s3:test",
    "deploy:production": "npm run production:build && npm run s3:production",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "clearCache": "jest --clearCache",
    "release": "npm-run-all clearCache lint build test:unit test:e2e",
    "auto-changelog": "auto-changelog  --hide-credit --commit-limit false --package --backfill-limit 0 --starting-version 3.0.0-beta.0",
    "npm-install-and-fix-aggrid": "npm i && chmod -R 777 ./AGGridFixer.js && node AGGridFixer.js"
  },
